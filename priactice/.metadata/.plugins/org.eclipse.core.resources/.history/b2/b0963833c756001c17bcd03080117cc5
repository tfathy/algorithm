package leetcode;

public class TwoSums {
	
	public static void main(String[] args) {
		int[] intArray = {2,9,7,15};
		TwoSums s = new TwoSums();
		s.printTwoSum(intArray,9);
	}
	
	void printTwoSum(int[] input, int target) {
		for(int i= 0; i< input.length-1; i++) {
			if(input[i]+input[i+1] == target) {
				System.out.println(input[i]+" " +input[i+1]) ;
			}
		}
	}
	

}
