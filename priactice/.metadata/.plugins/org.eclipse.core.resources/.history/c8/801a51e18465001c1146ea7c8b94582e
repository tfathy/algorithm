package miu;

public class AbernathyNumber {

	public static void main(String[] args) {
		AbernathyNumber a = new AbernathyNumber();
		System.out.println(a.isAbernathy(18));
	}
	
	int isAbernathy (int n) {
		
		int prime = isPrime(n);
		
		return prime;
		
	}
	
	int isPrime(int n) {
		int out =1;
		
		for(int i=2;i<n;i++) {
			if(n % i == 0) {
				return 0;
			}
		}
		return out;
	}
}
