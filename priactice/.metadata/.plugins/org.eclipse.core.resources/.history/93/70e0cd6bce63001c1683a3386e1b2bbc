package miu;

public class UpCount {

	
	public static void main(String[] args) {
		
	}
	 int upCount(int[] a,int n) {
		 if(a.length==0)  return 0;
		 
		 int total = 0;		  
		 for(int i= 0 ; i<a.length;i++){
			 if(i==0) {
				 if(a[i]>n) { // may be the first element in the array greater than n
					 total++;
				 }
			 }
			 
			 
		 }
		 
		
		
		 
		 return total;
	 }
}
